      },
      "package": "com.iamdjordjesimic.tiki"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/003395d2-1c6a-4996-9c67-f988af61a3c3"
